{
  "name": "h5-sdk",
  "version": "3.1.0",
  "description": "sdk.js 用于Web H5的sdk",
  "main": "dist/sdk.js",
  "types": "index.d.ts",
  "author": "Vace(ocdo@qq.com)",
  "scripts": {
    "dev": "rollup --config build/rollup.config.js --watch --environment BUILD:Web.Devlopment",
    "devlopment": "rollup --config build/rollup.config.js --environment BUILD:Web.Devlopment",
    "production": "rollup --config build/rollup.config.js --environment BUILD:Web.Production",
    "prepublish": "yarn devlopment && yarn production && node build/dts-generator.js",
    "docs": "typedoc --out docs --mode file --exclude \"**/*+(index.ts|.spec.ts|.e2e.ts|.js)\" --excludeNotExported src/web-entry.ts"
  },
  "files": [
    "index.d.ts",
    "dist/sdk-devlopment.js",
    "dist/sdk.js",
    "dist/sdk.css"
  ],
  "license": "MIT",
  "devDependencies": {
    "autoprefixer": "^9.4.9",
    "cssnano": "^4.1.10",
    "dts-generator": "^3.0.0",
    "less": "^3.9.0",
    "rollup": "^1.2.3",
    "rollup-plugin-commonjs": "8.3.0",
    "rollup-plugin-json": "^3.1.0",
    "rollup-plugin-node-resolve": "^4.0.1",
    "rollup-plugin-postcss": "1.2.7",
    "rollup-plugin-replace": "^2.1.0",
    "rollup-plugin-serve": "^1.0.1",
    "rollup-plugin-typescript2": "^0.19.3",
    "rollup-plugin-uglify": "^6.0.2",
    "tslib": "^1.9.3",
    "typescript": "^3.3.3333"
  },
  "dependencies": {
    "@types/blueimp-md5": "^2.7.0",
    "@types/store": "^2.0.1",
    "@types/zepto": "^1.0.30",
    "blueimp-md5": "^2.10.0",
    "es6-object-assign": "^1.1.0",
    "es6-promise": "^4.2.6",
    "store": "^2.0.12",
    "whatwg-fetch": "^3.0.0"
  }
}
