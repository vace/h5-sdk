{
  "name": "h5-sdk",
  "version": "3.3.1",
  "description": "sdk.js 用于Web/小程序的sdk",
  "main": "dist/sdk.js",
  "types": "index.d.ts",
  "author": "Vace(ocdo@qq.com)",
  "scripts": {
    "dev": "rollup --config build/rollup.config.js --watch --environment BUILD:web.Development",
    "development": "rollup --config build/rollup.config.js --environment BUILD:web.Development",
    "production": "rollup --config build/rollup.config.js --environment BUILD:web.Production",
    "mini": "rollup --config build/rollup.config.js --environment BUILD:mini.Production",
    "node": "rollup --config build/rollup.config.js --environment BUILD:node.Production",
    "prepublish": "yarn development && yarn production && node build/dts-generator.js && yarn mini",
    "test": "mocha --require ts-node/register --watch-extensions ts,tsx \"src/**/*.spec.ts\"",
    "docs": "typedoc --out docs --mode file --exclude \"**/*+(index.ts|.spec.ts|.e2e.ts|.js)\" --excludeNotExported src/web-entry.ts"
  },
  "files": [
    "index.d.ts",
    "dist/sdk.dev.js",
    "dist/sdk.js",
    "dist/sdk.mini.js",
    "dist/sdk.css"
  ],
  "license": "MIT",
  "devDependencies": {
    "@types/mocha": "^5.2.7",
    "autoprefixer": "^9.4.9",
    "cssnano": "^4.1.10",
    "dts-generator": "^3.0.0",
    "less": "^3.9.0",
    "mocha": "^6.1.4",
    "rollup": "^1.2.3",
    "rollup-plugin-commonjs": "8.3.0",
    "rollup-plugin-json": "^3.1.0",
    "rollup-plugin-node-resolve": "^4.0.1",
    "rollup-plugin-postcss": "1.2.7",
    "rollup-plugin-replace": "^2.1.0",
    "rollup-plugin-serve": "^1.0.1",
    "rollup-plugin-typescript2": "^0.19.3",
    "rollup-plugin-uglify": "^6.0.2",
    "tslib": "^1.9.3",
    "typescript": "^3.3.3333"
  },
  "dependencies": {
    "@types/blueimp-md5": "^2.7.0",
    "@types/chai": "^4.1.7",
    "@types/store": "^2.0.1",
    "@types/zepto": "^1.0.30",
    "blueimp-md5": "^2.10.0",
    "chai": "^4.2.0",
    "es6-object-assign": "^1.1.0",
    "es6-promise": "^4.2.6",
    "node-fetch": "^2.6.0",
    "ora": "^3.4.0",
    "ts-node": "^8.2.0",
    "whatwg-fetch": "^3.0.0"
  }
}
